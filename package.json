{
  "name": "iot2",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native start && react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --fix",
    "start": "react-native run-android && react-native start",
    "test": "jest",
    "prepare": "husky",
    "format": "prettier . -w ",
    "format:check": "prettier . -c",
    "build": "cd android ; ./gradlew assembleRelease",
    "detox:build": "cd android ; ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug",
    "detox:test": "detox test -c android.att.debug"
  },
